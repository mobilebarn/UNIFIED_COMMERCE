{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "NEXT_PUBLIC_APP_NAME": "Retail OS",
    "NEXT_PUBLIC_GRAPHQL_ENDPOINT": "https://retail-os-api.up.railway.app/graphql"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1.html",
      "status": 200
    },
    {
      "src": "/",
      "dest": "/index.html",
      "status": 200
    }
  ]
}